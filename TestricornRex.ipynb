    "k = Key('D', 'major')\n",
    "t = Time(4, 4, 90)\n",
    "s = Signature(k, t)\n",
    "song = Song('chord_hello_world_dmajor', 'JT', s)\n",
    "song.generate_chords(8)\n",
    "\n",
    "print(song)"
